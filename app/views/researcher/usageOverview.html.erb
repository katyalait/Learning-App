<div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Apps & Learning</h1>
            </div>
        </div>
		
		<div class="row">
			<div class="col-lg-8">
				<div class="form-group">
					<label>Sort by:</label>
					<label class="radio-inline">
						<input type="radio" name="optionsRadiosInline" id="optionsRadiosInline1" value="option1" checked>No. of Users
					</label>
					<label class="radio-inline">
						<input type="radio" name="optionsRadiosInline" id="optionsRadiosInline2" value="option2">Average Effectiveness
					</label>
					<label class="radio-inline">
						<input type="radio" name="optionsRadiosInline" id="optionsRadiosInline3" value="option3">Average Usage Duration
					</label>
				</div>
			</div>
		</div>
		
		
        <div class="row">
            <div class="col-lg-8">
                <div class="panel panel-default">
                    <div class="panel-heading">Summary Table</div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>App</th>
                                            <th>No. of Users</th>
						<th>Average Effectiveness %</th>
						<th>Average Usage Duration (Hours)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>1</td>
                                            <td>Snapchat</td>
                                            <td>122</td>
						<td>20</td>
						<td>2</td>
                                        </tr>
                                        <tr>
                                            <td>2</td>
                                            <td>Safari</td>
                                            <td>56</td>
						<td>67</td>
						<td>0.5</td>
                                        </tr>
                                        <tr>
                                            <td>3</td>
                                            <td>Twitter</td>
                                            <td>65</td>
						<td>51</td>
						<td>1.2</td>
                                        </tr>
					<tr>
                                            <td>4</td>
                                            <td>Trip Advisor</td>
                                            <td>60</td>
						<td>42</td>
						<td>0.1</td>
                                        </tr>
					<tr>
                                            <td>5</td>
                                            <td>Clock</td>
                                            <td>100</td>
						<td>22</td>
						<td>0.05</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</body>
</html>
    
